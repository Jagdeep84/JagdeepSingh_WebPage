@page
@model JagdeepSingh_webPage.Pages.RequestPageModel
@{
    ViewData["Title"] = "Request Page";
}
<section class="text-center">
    <h1 class="font-weight-bold">@ViewData["Title"]</h1>
</section>

<dl>

    <dt>Accept</dt>
    <dd>@Request.Headers["Accept"]</dd>
    <dt>Accept Encoding</dt>
    <dd>@Request.Headers["Accept-Encoding"]</dd>
    <dt>User Agent</dt>
    <dd>@Request.Headers["User-Agent"]</dd>
    <dt>Host</dt>
    <dd>@Request.Headers["Host"]</dd>

</dl>

