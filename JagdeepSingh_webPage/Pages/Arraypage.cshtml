@page
@model JagdeepSingh_webPage.Pages.ArraypageModel
@{
    ViewData["Title"] = "Top Authors";
    string[] names = new string[4];
    names[0] = "Jagdeep Singh";
    names[1] = "Gurdeep Singh";
    names[2] = "Amrit Mathur";
    names[3] = "Gagandeep Kaur";

}
<section class="text-center">
    <h1 class="font-weight-bold">@ViewData["Title"]</h1>
</section>
<ol>
    @for (int i = 0; i < names.Length; i++)
    {
        <li> @names[i] </li>
    }
  
</ol>
